<?xml version="1.0" encoding="utf-8"?>
<merge xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="78pt">

    <com.xiuxiu.confinement_nurse.ui.base.view.XTextView
        android:id="@+id/tv_layout_title"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginLeft="23pt"
        android:gravity="center"
        android:layout_weight="1"
        android:textColor="#000000"
        android:textSize="43pt">

    </com.xiuxiu.confinement_nurse.ui.base.view.XTextView>

    <com.xiuxiu.confinement_nurse.ui.base.view.XImageView
        android:id="@+id/tv_layout_tab"
        android:layout_width="33pt"
        android:layout_height="19pt"
        android:layout_marginLeft="23pt"
        android:layout_marginRight="24pt"
        app:bl_selected_drawable="@drawable/icon_down"
        app:bl_unSelected_drawable="@drawable/icon_down">

    </com.xiuxiu.confinement_nurse.ui.base.view.XImageView>
</merge>